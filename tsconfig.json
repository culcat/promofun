{
    "compilerOptions": {
      "target": "es6",
      "module": "esnext",
      "jsx": "react",
      "strict": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "baseUrl": ".",
      "paths": {
        "@mui/material": ["node_modules/@mui/material"]
      }
    },
    "include": ["src/**/*.ts", "src/**/*.tsx"],
    "exclude": ["node_modules"]
  }
  